/* Responsive Tabs */
(function ($) {
    function fixResponsiveTabs() {
        if ($(window).width() >= 960) {
            $('.tabs > ul').each(function (e) {
                var tabs = $(this);

                if (tabs.find('> li').length > 1) {
                    tabs.attr('style', 'display: table;');
                };
            });
        }
    }

    $(window).on('resize', function () {
        $('.tabs > ul').attr('style', '');
        fixResponsiveTabs();
    });

    fixResponsiveTabs();
})($);